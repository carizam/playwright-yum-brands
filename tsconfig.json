{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "lib": ["es6", "dom"],
      "esModuleInterop": true,
      "strict": true,
      "outDir": "./dist",
      "rootDir": "./",
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "baseUrl": "./",
      "paths": {
      "*": ["node_modules/*", "src/types/*"]
    }
    },
    "include": [
      
      "features/**/*.ts",
      "tests/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }